<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="black">#000000</color>
    <color name="code_box_bg_color">#FFE6E6E6</color>
    <color name="code_box_digit">#FFD0021B</color>
    <color name="code_box_place_holder_color">#FF9B9B9B</color>
    <color name="gray">#B4B4B4</color>
    <color name="gray_dark">#4a4a4a</color>
    <color name="white_darker">#F2F2F2</color>
    <declare-styleable name="CodeBoxView"><attr format="boolean" name="is_validity_icon_visible"/><attr format="integer" name="num_of_digits"/><attr format="reference" name="src_check_icon"/><attr format="reference" name="src_error_icon"/><attr format="color" name="place_holder_color"/><attr format="string" name="place_holder"/><attr format="string" name="spacer"/><attr format="color" name="bg_color"/><attr format="reference" name="bg"/><attr format="color" name="digit_color"/><attr format="string" name="allowed_characters"/></declare-styleable>
    <declare-styleable name="RoundedLinearLayout"><attr format="float" name="corner"/></declare-styleable>
    <dimen name="code_box_height">50dp</dimen>
    <dimen name="general_dialog_buttons_height">45dp</dimen>
    <dimen name="general_dialog_min_width">250dp</dimen>
    <dimen name="line_thickness">1dp</dimen>
    <dimen name="space_large">24dp</dimen>
    <dimen name="space_medium">16dp</dimen>
    <dimen name="space_medium_small">12dp</dimen>
    <dimen name="space_medium_xsmall">10dp</dimen>
    <dimen name="space_small">8dp</dimen>
    <dimen name="space_xlarge">32dp</dimen>
    <dimen name="space_xsmall">4dp</dimen>
    <dimen name="space_xsmall_large">6dp</dimen>
    <dimen name="space_xxlarge">44dp</dimen>
    <dimen name="space_xxsmall">2dp</dimen>
    <dimen name="space_xxxlarge">64dp</dimen>
    <string name="app_name">common</string>
    <style name="CodeBox">
        <item name="android:gravity">center</item>
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:textSize">24sp</item>
        <item name="android:fontFamily">serif-monospace</item>
    </style>
    <style name="md">
        <!-- EMPTY STYLE FOR FUTURE CUSTOMIZATION-->
    </style>
    <style name="md.SubTitle" parent="md.Title">
        <item name="android:layout_marginTop">@dimen/space_small</item>
        <item name="android:layout_marginBottom">@dimen/space_small</item>
        <item name="android:paddingTop">0dp</item>
        <item name="android:textColor">@color/gray</item>
    </style>
    <style name="md.SubTitleGraySmallerTextSize" parent="md.SubTitle">
        <item name="android:paddingTop">0dp</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">13dp</item>
    </style>
    <style name="md.Title" parent="md">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">@dimen/space_medium</item>
        <item name="android:paddingLeft">@dimen/space_medium</item>
        <item name="android:paddingRight">@dimen/space_medium</item>
        <item name="android:paddingBottom">@dimen/space_xsmall</item>
        <item name="android:textSize">15dp</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:textColor">@color/gray_dark</item>
    </style>
</resources>